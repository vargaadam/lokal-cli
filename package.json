{
  "name": "lokal-cli",
  "version": "0.3.0",
  "description": "lokal-cli",
  "author": "Adam Varga @vargaadam",
  "bin": {
    "lkl": "./bin/run"
  },
  "homepage": "https://github.com/vargaadam/lokal-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "vargaadam/lokal-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "cdk8s": "^1.5.61",
    "cdk8s-plus-22": "^1.0.0-beta.183",
    "constructs": "^3.3.257",
    "dotenv": "^16.0.0",
    "simple-git": "^3.6.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/jest": "^27.4.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.9.4",
    "cdk8s-cli": "^1.0.145",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "jest": "^27.5.1",
    "mock-fs": "^5.1.2",
    "oclif": "^2",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "oclif": {
    "bin": "lkl",
    "dirname": "lkl",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "clone": {
        "description": "Clone the repository if it's specified"
      },
      "generate": {
        "description": "Generate the required manifests"
      },
      "dev": {
        "description": "Start local dev"
      },
      "delete": {
        "description": "Delete the resources"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "pack": "oclif pack tarballs",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "jest --watchAll --no-cache",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "kubernetes",
    "development"
  ],
  "types": "dist/index.d.ts"
}
